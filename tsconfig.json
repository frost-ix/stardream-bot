{
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "preserveValueImports": false,
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "target": "ES2022",
        "strict": true,
        "outDir": "./dist",
        "forceConsistentCasingInFileNames": true
    },
    "ts-node": {
        "esm": true
    }
}
